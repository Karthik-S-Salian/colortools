{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
      "@data/*":["src/data/*"],
      "@layouts/*":["src/layouts/*"],
      "@styles/*":["src/styles/*"],
      "@lib/*":["src/lib/*"]
    }
  }
}